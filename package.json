{
  "name": "c2ft.github.io",
  "type": "module",
  "version": "0.0.1",
  "description": "My personal website built with Astro, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "author": "Prince Muel <68931805+princemuel@users.noreply.github.com>",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "build:nightly": "DEPLOY_CHANNEL=nightly astro build",
    "build:beta": "DEPLOY_CHANNEL=beta astro build",
    "build:stable": "DEPLOY_CHANNEL=stable astro build",
    "build:wasm": "./build-wasm.sh",
    "preview": "astro preview",
    "astro": "astro",
    "deploy:dev": "netlify dev",
    "deploy:nightly": "git push origin HEAD:nightly",
    "deploy:beta": "git push origin HEAD:beta",
    "promote:beta": "git checkout beta && git merge nightly && git push origin beta",
    "promote:stable": "git checkout main && git merge beta && git push origin main",
    "branch:feature": "git checkout -b",
    "branch:hotfix": "git checkout main && git checkout -b",
    "seed:all": "tsx --tsconfig ./tsconfig.scripts.json ./app/scripts/seed.ts"
  },
