generator client {
  provider = "prisma-client"
  output   = "../src"
}

datasource db {
  provider = "sqlite"
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  published Boolean @default(false)
}

model Comment {
  id   Int    @id @default(autoincrement())
  body String
}
